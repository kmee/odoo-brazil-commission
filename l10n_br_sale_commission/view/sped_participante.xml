<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="view_sped_participante_agent_form" model="ir.ui.view">
        <field name="name">sped.participante.agent.form</field>
        <field name="model">sped.participante</field>
        <field name="inherit_id" ref="l10n_br_base.sped_participante_base_form" />
        <field name="arch" type="xml">
            <field name="eh_cliente" position="after">
                <field name="agents"
                       attrs="{'invisible': [('eh_cliente', '=', False)]}"
                       widget="many2many_tags"/>
            </field>
        </field>
    </record>

</odoo>
